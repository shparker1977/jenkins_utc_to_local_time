{
    "name": "Convert Jenkins UTC to LocalTime",
    "version": "1.0",
    "description": "Convert Jenkins UTC time values to local time",
    "permissions": ["activeTab","declarativeContent", "storage"],
    "background": {
        "scripts": ["utctolocal.js"],
        "persistent": false
    },
    "page_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/clock16.png",
            "32": "images/clock32.png",
            "48": "images/clock48.png",
            "128": "images/clock128.png"
          }
    },
    "icons": {
        "16": "images/clock16.png",
        "32": "images/clock32.png",
        "48": "images/clock48.png",
        "128": "images/clock128.png"
    },
    "manifest_version": 2
}